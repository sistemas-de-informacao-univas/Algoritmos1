algoritmo "Questao_14"
// Função :
// Autor :
// Data : 26/03/2020
// Seção de Declarações 
var
   C, F : real
   

inicio
       escreval("Qual a temperatura em graus Celsius?")
       leia(C)
       
       F <- (C*9/5)+32
       
       escreval("A temperatura de ", C," em graus Celsius equivale a ", F," graus Fahrenheit")

fimalgoritmo
